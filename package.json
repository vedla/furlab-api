{
  "name": "furlab-api",
  "version": "1.0.0",
  "description": "Furlab API Services",
  "main": "dist/server.js",
  "repository": "https://github.com/vedla/furlab-api.git",
  "author": "Tay Fox <tay@vedla.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "docs": "npx jsdoc -c jsdoc.json",
    "swagger": "node src/swagger.ts",
    "proxy": "ts-node-dev src/server.ts",
    "workspace:install": "concurrently \"yarn workspace @furlab-api/image-daemon install\"",
    "dev": "concurrently \"yarn proxy\" \"yarn workspace @furlab-api/image-daemon dev\"",
    "dev:image-daemon": "yarn workspace @furlab-api/image-daemon dev",
    "build": "tsc -p tsconfig.json && yarn workspaces run build",
    "start": "concurrently \"node dist/server.js\" \"yarn workspace @furlab-api/image-daemon start\""
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "http-proxy-middleware": "^3.0.3",
    "good-logs": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "concurrently": "^9.1.0",
    "prettier": "^3.4.2",
    "openapi-types": "12.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
